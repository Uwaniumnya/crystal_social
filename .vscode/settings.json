{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.dart_tool": true,
    "**/build": true,
    "**/.flutter-plugins": true,
    "**/.flutter-plugins-dependencies": true,
    "**/node_modules": true,
    "**/.idea": true,
    "**/assets/tarot": true,
    "**/assets/shop": true,
    "**/assets/decorations": true,
    "**/assets/pets": true,
    "**/android/build": true,
    "**/ios/build": true,
    "**/windows/build": true,
    "**/linux/build": true,
    "**/macos/build": true,
    "**/web/build": true
  },
  "search.exclude": {
    "**/.git": true,
    "**/.dart_tool": true,
    "**/build": true,
    "**/node_modules": true,
    "**/assets/tarot": true,
    "**/assets/shop": true,
    "**/assets/decorations": true,
    "**/assets/pets": true,
    "**/*.lock": true,
    "**/.flutter-plugins": true,
    "**/.flutter-plugins-dependencies": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.dart_tool/**": true,
    "**/build/**": true,
    "**/assets/tarot/**": true,
    "**/assets/shop/**": true,
    "**/assets/decorations/**": true,
    "**/assets/pets/**": true
  },
  "dart.previewFlutterUiGuides": false,
  "dart.previewFlutterUiGuidesCustomTracking": false,
  "dart.flutterCreateIOSLanguage": "swift",
  "dart.flutterCreateAndroidLanguage": "kotlin",
  "dart.analysisServerFolding": false,
  "dart.enableSdkFormatter": true,
  "dart.lineLength": 100,
  "dart.showMainCodeLens": false,
  "dart.showTestCodeLens": false,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.tabCompletion": "on",
  "files.associations": {
    "*.dart": "dart"
  },
  "flutter.experiments.fastStart": true,
  "flutter.hotReloadOnSave": "manual",
  "typescript.preferences.includePackageJsonAutoImports": "off",
  "extensions.ignoreRecommendations": false,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "telemetry.telemetryLevel": "off",
  "workbench.startupEditor": "none"
}
